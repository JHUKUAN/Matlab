clear;clc
x=10*rand(1,100);
y=10*rand(1,100);
ii=0;
jj=0;
for i=1:100 
  if (x(i) >= 5 )
    ii    =ii+1;
    x2(ii)=x(i);
    y2(ii)=y(i);
   else
     jj=jj+1;
     x1(jj)=x(i);
     y1(jj)=y(i);
   end
end
      scatter(x1,y1,15,'r','filled') ;hold on
      scatter(x2,y2,15,'b','filled')
grid on
